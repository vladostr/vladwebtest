pipeline {
    agent any
    tools{
			maven 'myMaven-3'
			jdk 'myJDK-1.8'
		 }  
    stages {
        stage('Build') {
            when { branch 'master' }
            steps {
                echo '1. Build'
                sh 'mvn clean package'
            }
        }
        stage('Example Test') {
            when { branch 'master' }
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}    