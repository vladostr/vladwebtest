pipeline {
    agent any
    stages {
        stage('Build') {
            when { branch 'master' }
            steps {
                echo '1. Build'
                sh 'mvn clean package -f $WORKSPACE/vladwebtest/pom.xml'
            }
        }
        stage('Example Test') {
            when { branch 'master' }
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}    